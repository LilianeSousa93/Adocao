      "cell_type": "code",
      "source": [
        "cont =0\n",
        "acum=0\n",
        "n=int(input('Quantas pessoas compraram o produto X?\\n'))\n",
        "\n",
        "for i in range(n):\n",
        "  preco=float(input('Digite o preço do produto X\\n'))\n",
        "  acum+=preco\n",
        "\n",
        "  if preco>80:\n",
        "    cont+=1    #cont = cont + 1\n",
        "  \n",
        "media=acum/5\n",
        "print(f'\\nO valor médio é {media:.2f}\\n')\n",
        "print(f'{cont} pessoas pagaram a mais de R$80,00 no produto X\\n ')"
      ],
